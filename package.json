  "bugs": {
    "url": "https://github.com/NatalieChurch/Capstone_Back_End/issues"
  },
  "homepage": "https://github.com/NatalieChurch/Capstone_Back_End#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  }
}
